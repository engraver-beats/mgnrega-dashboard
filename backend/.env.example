# MGNREGA Dashboard Backend Configuration

# Server Configuration
PORT=5000
NODE_ENV=development

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:5173

# Real Data Integration
USE_REAL_DATA=true
MGNREGA_API_KEY=your_api_key_here
DATA_GOV_IN_API_KEY=your_api_key_here
DATAFUL_API_KEY=your_api_key_here

# Cache Configuration
CACHE_TTL=3600

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Database (if needed for persistent caching)
# MONGODB_URI=mongodb://localhost:27017/mgnrega_dashboard

# Logging
LOG_LEVEL=info
